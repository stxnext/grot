window.cfg =
  ###
  Board size
  ###
  defaultBoardSize: 4

  ###
  Enables or disables possibility to pass
  initial board size as ?size=x param
  ###
  customBoardSize: true

  ###
  FieldWidget(Circle) settings
  ###
  circleColor1: '#95a5a6'
  circleColor2: '#2980b9'
  circleColor3: '#27ae60'
  circleColor4: '#e74c3c'

  circleRadius: 45

  spaceBetweenFields: 1
  ###
  Arrow settings
  ###
  arrowColor: '#333333'

  ###
  Tween duration
  ###
  tweenDuration: 0.5

  ###
  Font settings
  ###
  fontColor: '#ecf0f1'
  fontFamily: 'Courier New'
  fontScoMovSize: 8
  fontRestSize: 6

  ###
  Background color settings
  ###
  bodyColor: '#333333'
  gameOverMessageColor: 'green'

  ###
  Urls settings
  ###
  scoreBoardLink: 'http://stxnext.pl/'
